<div class="form-new">
  <h1>New Entry for "<%= @list.name %>"</h1>
  <%= simple_form_for [@list, @bookmark] do |f| %>
    <%= f.input :comment, as: :text %>
    <%= f.association :movie %>
    <%= f.submit %>
  <% end %>
</div>

<div class="show-buttons">
  <%= link_to "Back to List", list_path(@list), class:"btn btn-primary form-buttons" %>
</div>

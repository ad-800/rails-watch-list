<div class="index-container">
  <h1>Movies</h1>
  <div class="container text-center">
    <div class="row">
      <% @movies.each do |movie| %>
        <img src="<%= movie.poster_url %>" alt="movie poster for <%= movie.title %>">
        <h3><%= movie.title %></h3>
        <p><%= movie.rtscore %></p>
        <p><%= movie.mcscore %></p>
      <% end %>
    </div>
  </div>
</div>
